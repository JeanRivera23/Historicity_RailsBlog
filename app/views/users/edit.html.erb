<h1>Edit Profile</h1>

<%= form_for(@user) do |f| %>

  <%= f.label "First Name" %>
  <%= f.text_field :fname %>

  <%= f.label "Last Name" %>
  <%= f.text_field :lname %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.submit %>

<% end %>


